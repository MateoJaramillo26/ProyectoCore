{
    "version": 2,
    "builds": [
        {
            "src": "OrientacionVocacional/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "OrientacionVocacional/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "OrientacionVocacional.settings",
        "ALLOWED_HOSTS": ".vercel.app"
    }
}